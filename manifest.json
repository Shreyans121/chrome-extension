{
  "name": "Chronus",
  "description": "Time Tracker",
  "version": "1",

  "permissions": [
      "tabs",
      "storage"
  ],

  "background": {
      "scripts": ["js/jquery.min.js"]
  },

  "chrome_url_overrides":{
      "newtab" : "index.html"
  },

  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
